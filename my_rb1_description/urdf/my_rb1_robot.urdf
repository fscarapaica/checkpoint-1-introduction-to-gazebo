<?xml version="1.0" ?>

<robot name="robot">

    <!-- Base -->
        <!-- Link - base -->
        <link name="base_link">
            <inertial>
                <mass value="21" />
                <origin xyz="0 0 0" rpy="0 0 0" />
                <inertia ixx="1.5417" ixy="0" ixz="0" iyy="3.467" iyz="0" izz="4.742" />
            </inertial>

            <collision>
                <geometry>
                    <cylinder length="0.3" radius="0.25"/>
                </geometry>
            </collision>

            <visual>
                <geometry>
                    <cylinder length="0.3" radius="0.25"/>
                </geometry>
                <material name="Yellow">
                    <color rgba="1 1 0 1" />
                </material>
            </visual>
        </link>

        <!-- Link - base footprint -->
        <link name="base_footprint">
            <inertial>
                <mass value="21" />
                <origin xyz="0 0 0" rpy="0 0 0" />
                <inertia ixx="1.5417" ixy="0" ixz="0" iyy="3.467" iyz="0" izz="4.742" />
            </inertial>
        </link>

        <!-- Joint - joint base base footprint -->
        <joint name="joint_base_base_footprint" type="fixed">
            <origin rpy="0 0 0" xyz="0 0 -0.15" />
            <child link="base_footprint" />
            <parent link="base_link" />
            <joint_properties damping="1.0" friction="1.0" />
        </joint>

    <!-- Wheels -->
        <!-- Link - left wheel -->
        <link name="left_wheel">
            <inertial>
                <mass value="1" />
                <origin xyz="0 0 0" rpy="0 0 0" />
                <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0" izz="0.005"/>
            </inertial>

            <collision>
                <origin rpy="1.57079633 0 0" xyz="0 0 0" />
                <geometry>
                    <cylinder length="0.02" radius="0.025"/>
                </geometry>
            </collision>
            <visual>
                <origin rpy="1.57079633 0 0" xyz="0 0 0" />
                <geometry>
                    <cylinder length="0.02" radius="0.025"/>
                </geometry>
            </visual>
        </link>

        <!-- Joint - joint base left wheel -->
        <joint name="joint_base_left_wheel" type="continuous">
            <origin rpy="0 0 0" xyz="0 -0.2 -0.15" />
            <child link="left_wheel" />
            <parent link="base_link" />
            <axis rpy="0 0 0" xyz="0 1 0" />
            <limit effort="10000" velocity="1000" />
            <joint_properties damping="1.0" friction="1.0" />
        </joint>

        <!-- Link - right wheel -->
        <link name="right_wheel">
            <inertial>
                <mass value="1" />
                <origin xyz="0 0 0" rpy="0 0 0" />
                <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0" izz="0.005"/>
            </inertial>

            <collision>
                <origin rpy="1.57079633 0 0" xyz="0 0 0" />
                <geometry>
                    <cylinder length="0.02" radius="0.025"/>
                </geometry>
            </collision>
            <visual>
                <origin rpy="1.57079633 0 0" xyz="0 0 0" />
                <geometry>
                    <cylinder length="0.02" radius="0.025"/>
                </geometry>
            </visual>
        </link>

        <!-- Joint - joint base right wheel -->
        <joint name="joint_base_right_wheel" type="continuous">
            <origin rpy="0 0 0" xyz="0 0.2 -0.15" />
            <child link="right_wheel" />
            <parent link="base_link" />
            <axis rpy="0 0 0" xyz="0 1 0" />
            <limit effort="10000" velocity="1000" />
            <joint_properties damping="1.0" friction="1.0" />
        </joint>

    <!-- Caster wheels -->
        <!-- Link - front caster wheel -->
        <link name="front_caster">
            <inertial>
                <mass value="1" />
                <origin xyz="0 0 0" rpy="0 0 0" />
                <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0" izz="0.005"/>
            </inertial>

            <collision>
                <origin rpy="0 0 0" xyz="0 0 0" />
                <geometry>
                    <sphere radius="0.02"/>
                </geometry>
            </collision>
            <visual>
                <origin rpy="0 0 0" xyz="0 0 0" />
                <geometry>
                    <sphere radius="0.02"/>
                </geometry>
            </visual>

            <collision>
                <origin rpy="0 0 0" xyz="0 0 -0.015" />
                <geometry>
                    <sphere radius="0.01"/>
                </geometry>
            </collision>
            <visual>
                <origin rpy="0 0 0" xyz="0 0 -0.015" />
                <geometry>
                    <sphere radius="0.01"/>
                </geometry>
            </visual>
        </link>

        <!-- Joint - joint base front caster -->
        <joint name="joint_base_front_caster" type="fixed">
            <origin rpy="0 0 0" xyz="0.1 0 -0.15" />
            <child link="front_caster" />
            <parent link="base_link" />
            <joint_properties damping="1.0" friction="1.0" />
        </joint>

        <!-- Link - back caster wheel -->
        <link name="back_caster">
            <inertial>
                <mass value="1" />
                <origin xyz="0 0 0" rpy="0 0 0" />
                <inertia ixx="0.002526666666667" ixy="0" ixz="0" iyy="0.002526666666667" iyz="0" izz="0.005"/>
            </inertial>

            <collision>
                <origin rpy="0 0 0" xyz="0 0 0" />
                <geometry>
                    <sphere radius="0.02"/>
                </geometry>
            </collision>
            <visual>
                <origin rpy="0 0 0" xyz="0 0 0" />
                <geometry>
                    <sphere radius="0.02"/>
                </geometry>
            </visual>

            <collision>
                <origin rpy="0 0 0" xyz="0 0 -0.015" />
                <geometry>
                    <sphere radius="0.01"/>
                </geometry>
            </collision>
            <visual>
                <origin rpy="0 0 0" xyz="0 0 -0.015" />
                <geometry>
                    <sphere radius="0.01"/>
                </geometry>
            </visual>
        </link>

        <!-- Joint - joint base back caster -->
        <joint name="joint_base_back_caster" type="fixed">
            <origin rpy="0 0 0" xyz="-0.1 0 -0.15" />
            <child link="back_caster" />
            <parent link="base_link" />
            <joint_properties damping="1.0" friction="1.0" />
        </joint>

    <!-- Sensors -->
            <!-- Link - front laser -->
        <link name="front_laser">
            <inertial>
                <mass value="21" />
                <origin xyz="0 0 0" rpy="0 0 0" />
                <inertia ixx="1.5417" ixy="0" ixz="0" iyy="3.467" iyz="0" izz="4.742" />
            </inertial>
        </link>

        <!-- Joint - joint base front laser -->
        <joint name="joint_base_front_laser" type="fixed">
            <origin rpy="3.14159 0 0" xyz="0.25 0 0.075" />
            <child link="front_laser" />
            <parent link="base_link" />
            <joint_properties damping="1.0" friction="1.0" />
        </joint>
        
</robot>